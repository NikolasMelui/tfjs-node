{
  "name": "@tensorflow/tfjs-node",
  "version": "1.1.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "gypfile": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/tensorflow/tfjs-node.git"
  },
  "engines": {
    "node": ">=8.11.0"
  },
  "scripts": {
    "build": "tsc",
    "build-npm": "./scripts/build-npm.sh",
    "clean-deps": "rm -rf deps",
    "coverage": "nyc ts-node src/run_tests.ts",
    "enable-gpu": "node scripts/install.js gpu download && yarn",
    "format": "clang-format -i -style=Google binding/*.cc binding/*.h",
    "install-local": "node scripts/install.js",
    "install": "./scripts/user-install.sh",
    "link-local": "yalc link",
    "lint": "tslint -p . -t verbose",
    "prep": "cd node_modules/@tensorflow/tfjs-core && yarn && yarn build",
    "publish-local": "yarn prep && yalc push",
    "test": "ts-node src/run_tests.ts",
    "test-ci": "./scripts/test-ci.sh",
    "test-ts-integration": "./scripts/test-ts-integration.sh",
    "upload-binary": "./scripts/compress-and-upload-binary.sh"
  },
  "devDependencies": {
    "@types/bindings": "~1.3.0",
    "@types/jasmine": "~2.8.6",
    "@types/node": "^10.5.1",
    "@types/progress": "^2.0.1",
    "@types/rimraf": "~2.0.2",
    "clang-format": "~1.2.2",
    "jasmine": "~3.1.0",
    "node-fetch": "^2.3.0",
    "nyc": "^13.3.0",
    "shelljs": "^0.8.3",
    "tmp": "^0.0.33",
    "ts-node": "^5.0.1",
    "tslint": "~5.9.1",
    "typescript": "3.3.3333",
    "yalc": "~1.0.0-pre.21"
  },
  "dependencies": {
    "@tensorflow/tfjs": "~1.1.2",
    "adm-zip": "^0.4.11",
    "bindings": "~1.3.0",
    "https-proxy-agent": "^2.2.1",
    "progress": "^2.0.0",
    "rimraf": "^2.6.2",
    "tar": "^4.4.6",
    "node-pre-gyp": "0.13.0"
  },
  "binary": {
    "module_name": "tfjs_binding",
    "module_path": "./lib/binding/napi-v{napi_build_version}",
    "host": "https://storage.googleapis.com/tf-builds/pre-built-binary",
    "remote_path": "./napi-v{napi_build_version}/{version}/",
    "package_name": "CPU-darwin-1.1.2.tar.gz",
    "napi_versions": [
      3
    ]
  }
}
